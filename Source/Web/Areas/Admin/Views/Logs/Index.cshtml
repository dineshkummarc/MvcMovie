@{
    ViewBag.Title = "Index";
}

<h2>Index</h2> 

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>Summary</th> 
        <th>Level</th> 
        <th>IpAddress</th> 
        <th>Server</th>
        <th>Session</th>
        <th>Type</th>
        <th>Date</th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr> 
        <td>@item.Summary</td> 
        <td>@item.Level</td> 
        <td>@item.IpAddress</td>  
        <td>@item.Server</td> 
        <td>@item.Session</td> 
        <td>@item.Type</td>  
        <td>@item.UpdatedAt</td>  
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 

        </td>
    </tr>
}

</table>
